{
    "name":"riverline/worker-bundle",
    "description":"Symfony Bundle to use queue with workers",
    "keywords":["worker", "queue", "bundle", "symfony"],
    "type":"library",
    "license":"MIT",
    "authors":[
        {
            "name":"Romain Cambien",
            "email":"romain@riverline.fr"
        },
        {
            "name":"Sebastien Porati",
            "email":"sebastien@riverline.fr"
        },
        {
            "name":"Riverline",
            "homepage":"http://www.riverline.fr"
        }
    ],
    "homepage": "https://github.com/eresults/worker-bundle",
    "require":{
        "php":                          ">=7.4",
        "ext-zlib":                     "*",
        "symfony/console":              "^4.0|^5.0",
        "symfony/dependency-injection": "^4.0|^5.0",
        "symfony/config":               "^4.0|^5.0",
        "symfony/http-kernel":          "^4.0|^5.0"
    },
    "suggest": {
        "aws/aws-sdk-php":                   "^3.0",
    },
    "require-dev": {
        "phpunit/phpunit":     "^9.0",
        "aws/aws-sdk-php":     "^3.142"
    },
    "autoload":{
        "psr-4":{
            "Riverline\\WorkerBundle\\": "src/"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "Riverline\\WorkerBundle\\": "tests/"
        }
    }
}
